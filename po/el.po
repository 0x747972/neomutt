"<file> [...] --] <addr> [...]\n"
"       mutt [<options>] [-x] [-s <subj>] [-bc <addr>] [-a <file> [...] --] "
"<addr> [...] < message\n"
"       mutt [<options>] -p\n"
"       mutt [<options>] -A <alias> [...]\n"
"       mutt [<options>] -Q <query> [...]\n"
"       mutt [<options>] -B\n"
"       mutt [<options>] -D [-S]\n"
"       mutt -v[v]\n"
